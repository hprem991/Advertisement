<?xml version="1.0" encoding="utf-8"?>
<!DOCTYPE hibernate-mapping PUBLIC
 "-//Hibernate/Hibernate Mapping DTD//EN"
 "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
<class name="Advertisement" table="Advertisement">
	<meta attribute="class-description"> This class contains the Advertisement detail.
	</meta>
	<id name="id" type="int" column="ID">
		<generator class="native"/> </id>
	<property name="latitute" column="LATITUTE" type="string"/> 
	<property name="longitute" column="LONGITITUTE" type="string"/> 
	<property name="time" column="TIME" type="string"/> 
 </class>
</hibernate-mapping>